{
    "terminals": {
        "PLUS": "+",
        "WHILE": "while",
        "IF": "if",
        "LPAREN": "(",
        "RPAREN": ")",
        "LBRACE": "{",
        "RBRACE": "}",
        "NEWLINE": "\n"
    },
    "nonterminals": {
        "start": [{
                "production": ["stmts"]
            },
            {
                "production": ["if_stmt"]
            }
        ],
        "if_stmt": [{
            "production": ["IF", "LPAREN", "components", "RPAREN", "LBRACE", "stmt", "RBRACE"],
            "display": "@ @@@ @\n\t@\n@"
        }],
        "stmts": {
            "child": "stmt",
            "separator": "NEWLINE"
        },
        "stmt": [{
            "production": ["IDENTIFIER"]
        }],
        "test_nonterm": [{
            "production": ["components"]
        }],
        "components": {
            "child": "component",
            "separator": "PLUS"
        },
        "component": [{
                "production": ["IDENTIFIER"]
            },
            {
                "production": ["NUMERAL"]
            }
        ]
    }
}