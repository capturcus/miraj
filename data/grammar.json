{
    "terminals": {
        "PLUS": "+",
        "WHILE": "while",
        "LPAREN": "(",
        "RPAREN": ")",
        "LBRACE": "{",
        "RBRACE": "}",
        "IF": "}",
        "NEWLINE": "\n"
    },
    "nonterminals": {
        "start": [{
                "production": ["stmts"]
            },
            {
                "production": ["if_stmt"]
            }
        ],
        "if_stmt": [{
            "production": ["IF", "LPAREN", "components", "RPAREN", "LBRACE", "stmts", "RBRACE"],
            "display": "if (@2) {\n@@5\n}"
        }],
        "stmts": {
            "child": "stmt",
            "separator": "NEWLINE"
        },
        "stmt": [{
            "production": ["IDENTIFIER"]
        }],
        "components": {
            "child": "component",
            "separator": "PLUS"
        },
        "component": [{
                "production": ["IDENTIFIER"]
            },
            {
                "production": ["NUMERAL"]
            }
        ]
    }
}